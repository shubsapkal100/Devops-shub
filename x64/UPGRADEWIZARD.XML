        <Button ID="Next" Enabled="false"/>
      </Buttons>
      <ControllerType>Microsoft.SqlServer.Configuration.InstallWizard.ReportingServicesMigrationController, Microsoft.SqlServer.Configuration.InstallWizard</ControllerType>
    </Page>        
      
    <Page
      ID="FeatureTree">
        <Title ResourceID="UpgradeFeatureTreeTitle"></Title>
        <Subtitle ResourceID="UpgradeFeatureTreeSubtitle"></Subtitle>
        <Buttons>
            <Button ID="Back" Enabled="true"/>
            <Button ID="Finish" Visible="false"/>
        </Buttons>
        <ControllerType>Microsoft.SqlServer.Configuration.InstallWizard.UpgradeFeatureSelectionController, Microsoft.SqlServer.Configuration.InstallWizard</ControllerType>
    </Page>

    <Page
        ID="AdvancedAnalyticsSetup" PageType="Page">
        <Title ResourceID="AdvancedAnalyticsSetupTitle"></Title>
        <Subtitle ResourceID="AdvancedAnalyticsSetupSubtitle"></Subtitle>
